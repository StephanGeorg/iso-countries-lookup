{
  "name": "iso-countries-lookup",
  "version": "2.0.4",
  "description": "Find country codes by country names in multiple languages.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/register",
    "build": "babel src --presets @babel/preset-env --plugins @babel/plugin-transform-runtime --out-dir dist --copy-files",
    "prepublish": "npm run build"
  },
  "keywords": [
    "country codes",
    "countries",
    "iso codes",
    "iso country code",
    "ISO 3166",
    "ISO 3166-1 alpha-2",
    "ISO 3166-1 alpha-1"
  ],
  "author": "Stephan Georg <s.georg@hey.com>",
  "license": "MIT",
  "repository": "github:StephanGeorg/iso-countries-lookup",
  "bugs": {
    "url": "https://github.com/StephanGeorg/iso-countries-lookup/issues"
  },
  "homepage": "https://github.com/StephanGeorg/iso-countries-lookup#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "i18n-iso-countries": "^6.8.0",
    "slugify": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "lodash.uniq": "^4.5.0",
    "mocha": "^9.0.3",
    "world-countries": "^4.0.0"
  },
  "engines": {
    "node": ">=8"
  }
}
