{
  "name": "iso-countries-lookup",
  "version": "2.0.8",
  "description": "Find country codes by country names in multiple languages.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/register",
    "build": "babel src --presets @babel/preset-env --plugins @babel/plugin-transform-runtime --out-dir dist --copy-files",
    "prepublish": "npm run build"
  },
  "keywords": [
    "country codes",
    "countries",
    "iso codes",
    "iso country code",
    "ISO 3166",
    "ISO 3166-1 alpha-2",
    "ISO 3166-1 alpha-1"
  ],
  "author": "Stephan Georg <s.georg@hey.com>",
  "license": "MIT",
  "repository": "github:StephanGeorg/iso-countries-lookup",
  "bugs": {
    "url": "https://github.com/StephanGeorg/iso-countries-lookup/issues"
  },
  "homepage": "https://github.com/StephanGeorg/iso-countries-lookup#readme",
  "dependencies": {
    "@babel/runtime": "^7.23.5",
    "i18n-iso-countries": "^7.7.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/node": "^7.22.6",
    "@babel/eslint-parser": "^7.22.7",
    "@babel/plugin-transform-runtime": "^7.22.7",
    "@babel/preset-env": "^7.22.7",
    "@babel/register": "^7.22.5",
    "chai": "^4.3.7",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "world-countries": "^5.0.0"
  },
  "engines": {
    "node": ">=8"
  }
}
