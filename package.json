{
  "name": "iso-countries-lookup",
  "version": "2.0.0",
  "description": "Find country codes by country names in multiple languages.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/register",
    "build": "babel src --presets @babel/preset-env --plugins @babel/plugin-transform-runtime --out-dir dist --copy-files",
    "prepublish": "npm run build"
  },
  "keywords": [
    "country codes",
    "countries",
    "iso codes",
    "iso country code",
    "ISO 3166",
    "ISO 3166-1 alpha-2",
    "ISO 3166-1 alpha-1"
  ],
  "author": "Stephan Georg <s.georg@nearest.place>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephanGeorg/iso-countries-lookup.git"
  },
  "bugs": {
    "url": "https://github.com/StephanGeorg/iso-countries-lookup/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "i18n-iso-countries": "^6.5.0",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/register": "^7.13.8",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "lodash.uniq": "^4.5.0",
    "mocha": "^8.3.2",
    "world-countries": "^4.0.0"
  }
}
