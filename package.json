{
  "name": "iso-countries-lookup",
  "version": "1.0.14",
  "description": "Find country codes by country names in multiple languages.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --require @babel/register",
    "build": "babel src --presets @babel/preset-env --plugins @babel/plugin-transform-runtime --out-dir dist --copy-files",
    "prepublish": "npm run build"
  },
  "keywords": [
    "country codes",
    "countries",
    "iso codes",
    "iso country code",
    "ISO 3166",
    "ISO 3166-1 alpha-2",
    "ISO 3166-1 alpha-1"
  ],
  "author": "Stephan Georg <s.georg@nearest.place>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephanGeorg/iso-countries-lookup.git"
  },
  "bugs": {
    "url": "https://github.com/StephanGeorg/iso-countries-lookup/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "i18n-iso-countries": "^4.3.1",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "lodash.uniq": "^4.5.0",
    "mocha": "^7.1.1"
  }
}
